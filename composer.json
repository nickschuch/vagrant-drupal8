{
  "name": "nickschuch/vd8",
  "description": "Vagrant for Drupal 8",
  "require": {
    "drush/drush": "dev-master",
    "phing/phing": "~2.12",
    "squizlabs/php_codesniffer": "~2.3",
    "drupal/coder": "~8.2"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "bin-dir": "bin/"
  },
  "installer-paths": {
    "vendor/drupal/{$name}": ["drupal/coder"]
  }
}
